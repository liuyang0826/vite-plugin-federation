    "@babel/types": "^7.22.10",
    "conventional-changelog-cli": "^2.1.1",
    "esbuild": "^0.19.2",
    "typescript": "^4.9.4",
    "vite": "^4.4.9"
  },
  "peerDependencies": {
    "vite": ">=4.4.8"

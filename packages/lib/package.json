    "node": ">=14.0.0",
    "pnpm": ">=7.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/originjs/vite-plugin-federation.git"
  },
  "keywords": [
    "vite",
    "plugins"
  ],
  "author": "@liuyang0826",
  "publishConfig": {
    "access": "public"
  },
  "license": "MulanPSL-2.0",
  "bugs": {
    "url": "https://github.com/originjs/vite-plugin-federation/issues"
